#include <iostream>
#include <unordered_map>
using namespace std;

int main() {
    string s;
    cin >> s;
    unordered_map<char, int> freq;
    for (char c : s)
        freq[c]++;

    for (auto it : freq) {
        if (it.second > 1)
            cout << it.first << " " << it.second << endl;
    }

    return 0;
}
