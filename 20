#include <iostream>
#include <string>
using namespace std;

string getKeypadSequence(char c) {
    string keys[] = {"2","22","222","3","33","333","4","44","444","5","55","555",
                     "6","66","666","7","77","777","7777","8","88","888","9","99","999","9999"};
    if (c >= 'a' && c <= 'z') return keys[c - 'a'];
    if (c >= 'A' && c <= 'Z') return keys[c - 'A'];
    if (c == ' ') return "0";
    return "";
}

int main() {
    string s;
    getline(cin, s);
    for (char c : s)
        cout << getKeypadSequence(c);
    return 0;
}
