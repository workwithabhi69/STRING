#include <iostream>
#include <vector>
#include <unordered_set>
using namespace std;

bool wordBreak(string A, unordered_set<string>& B) {
    int n = A.size();
    vector<bool> dp(n + 1, false);
    dp[0] = true;
    for (int i = 1; i <= n; i++) {
        for (int j = 0; j < i; j++) {
            if (dp[j] && B.find(A.substr(j, i - j)) != B.end()) {
                dp[i] = true;
                break;
            }
        }
    }
    return dp[n];
}

int main() {
    string A;
    int n;
    cin >> A >> n;
    unordered_set<string> B;
    for (int i = 0; i < n; i++) {
        string word;
        cin >> word;
        B.insert(word);
    }
    if (wordBreak(A, B))
        cout << "Yes";
    else
        cout << "No";
    return 0;
}
