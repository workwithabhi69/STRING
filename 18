#include <iostream>
#include <string>
using namespace std;

void search(string pat, string txt) {
    int n = txt.size(), m = pat.size();
    for (int i = 0; i <= n - m; i++) {
        int j;
        for (j = 0; j < m; j++)
            if (txt[i + j] != pat[j])
                break;
        if (j == m)
            cout << "Pattern found at index " << i << endl;
    }
}

int main() {
    string txt, pat;
    cin >> txt >> pat;
    search(pat, txt);
    return 0;
}
