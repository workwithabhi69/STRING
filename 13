#include <iostream>
#include <vector>
#include <climits>
using namespace std;

void neatPrint(vector<int> nums, int K) {
    int n = nums.size();
    vector<vector<int>> extras(n, vector<int>(n));
    vector<vector<int>> lc(n, vector<int>(n));
    vector<int> c(n);
    vector<int> p(n);

    for (int i = 0; i < n; i++) {
        extras[i][i] = K - nums[i];
        for (int j = i + 1; j < n; j++)
            extras[i][j] = extras[i][j - 1] - nums[j] - 1;
    }

    for (int i = 0; i < n; i++) {
        for (int j = i; j < n; j++) {
            if (extras[i][j] < 0)
                lc[i][j] = INT_MAX;
            else
                lc[i][j] = extras[i][j] * extras[i][j];
        }
    }

    c[0] = lc[0][0];
    for (int j = 1; j < n; j++) {
        c[j] = lc[0][j];
        p[j] = 0;
        for (int i = 1; i <= j; i++) {
            if (c[i - 1] != INT_MAX && lc[i][j] != INT_MAX && c[i - 1] + lc[i][j] < c[j]) {
                c[j] = c[i - 1] + lc[i][j];
                p[j] = i;
            }
        }
    }

    vector<pair<int, int>> lines;
    int j = n - 1;
    while (j >= 0) {
        int i = p[j];
        lines.push_back({i, j});
        j = i - 1;
    }

    for (int k = lines.size() - 1; k >= 0; k--) {
        for (int i = lines[k].first; i <= lines[k].second; i++)
            cout << nums[i] << " ";
        cout << endl;
    }
}

int main() {
    int n, K;
    cin >> n;
    vector<int> nums(n);
    for (int i = 0; i < n; i++)
        cin >> nums[i];
    cin >> K;
    neatPrint(nums, K);
    return 0;
}
