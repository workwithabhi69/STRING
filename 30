#include <iostream>
#include <string>
using namespace std;

int minSwaps(string s) {
    int count = 0, swaps = 0;
    for (char c : s) {
        if (c == '[') count++;
        else count--;
        if (count < 0) {
            swaps += -count;
            count = 0;
        }
    }
    return swaps;
}

int main() {
    string s;
    cin >> s;
    cout << minSwaps(s);
    return 0;
}
