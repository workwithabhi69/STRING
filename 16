#include <iostream>
#include <stack>
using namespace std;

bool isBalanced(string exp) {
    stack<char> s;
    for (char c : exp) {
        if (c == '(' || c == '{' || c == '[')
            s.push(c);
        else {
            if (s.empty()) return false;
            if ((c == ')' && s.top() != '(') ||
                (c == '}' && s.top() != '{') ||
                (c == ']' && s.top() != '['))
                return false;
            s.pop();
        }
    }
    return s.empty();
}

int main() {
    string exp;
    cin >> exp;
    if (isBalanced(exp))
        cout << "true";
    else
        cout << "false";
    return 0;
}
