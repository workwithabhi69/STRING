#include <iostream>
#include <vector>
using namespace std;

void printSubsequences(string s) {
    int n = s.length();
    int total = 1 << n;
    for (int i = 1; i < total; i++) {
        string subseq = "";
        for (int j = 0; j < n; j++) {
            if (i & (1 << j))
                subseq += s[j];
        }
        cout << subseq << endl;
    }
}

int main() {
    string s;
    cin >> s;
    printSubsequences(s);
    return 0;
}
